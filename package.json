{
  "name": "useful-tools-chrome-extension",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite -c vite.popup.config.ts",
    "build": "node --no-warnings ./scripts/build.js",
    "build:prod": "vue-tsc && vite build -c vite.popup.config.js && vite build -c vite.content.config.js && vite build -c vite.background.config.js && node ./scripts/afterBuild.js --mode production",
    "content": "tsc --watch",
    "serve": "vite preview"
  },
  "dependencies": {
    "codeflask": "^1.4.1",
    "mammoth": "^1.9.1",
    "pinia": "^3.0.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/ws": "^8.18.1",
    "@vicons/ionicons5": "^0.13.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/tsconfig": "^0.7.0",
    "archiver": "^7.0.1",
    "chokidar": "^4.0.3",
    "chrome-types": "^0.1.352",
    "concurrently": "^9.2.0",
    "inquirer": "^12.6.3",
    "naive-ui": "^2.41.0",
    "ora": "^8.2.0",
    "sass-embedded": "^1.89.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.1.1",
    "vue-tsc": "^2.2.8",
    "ws": "^8.18.3"
  }
}
